{
    "name": "flutter_realtime",
    "version": "1.0.0",
    "description": "A new Flutter project.",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "build": "flutter pub run build_runner build --delete-conflicting-outputs",
        "clean": "flutter clean",
        "watch": "dart run build_runner watch -d",
        "get": "flutter pub upgrade",
        "release": "flutter build appbundle --release",
        "verify": "keytool -list -v -keystore android/app/keystore-release.jks -alias upload",
        "keyhash": "keytool -exportcert -alias upload -keystore android/app/keystore-release.jks | openssl sha1 -binary | openssl base64"
    },
    "keywords": [],
    "author": "",
    "license": "ISC"
}
